// Prisma schema file,
// docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}



model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String

  @@map("users") // Maps the model to the 'users' table
}

model Option {
  id        Int @id @default(autoincrement())
  title     String @unique 
  active    Boolean 
  completed Boolean
}